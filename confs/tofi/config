	font = "Monospace"
	font-size = 10
	font-features = ""
	font-variations = ""
	hint-font = true

	text-color = #aaaaaa

	prompt-background = #00000000
	prompt-background-padding = 0
	prompt-background-corner-radius = 0

	placeholder-color = #FFFFFFA8
	placeholder-background = #00000000
	placeholder-background-padding = 0
	placeholder-background-corner-radius = 0

	# Input text theme
	input-color = #9a9a9a
	input-background = #00000000
	input-background-padding = 0
	input-background-corner-radius = 0

	# Default result text theme
	# default-result-color = #FFFFFF
	default-result-background = #00000000
	default-result-background-padding = 0
	default-result-background-corner-radius = 0

	# Selection text
	selection-color = #00007f
	selection-background = #b0b0b0
	selection-background-padding = 2
	selection-background-corner-radius = 5

	# Matching portion of selection text
	selection-match-color = #00000000

    # Text cursor theme
	text-cursor-style = bar
	# Corner radius of the text cursor
	text-cursor-corner-radius = 0

	# Prompt to display.
	prompt-text = "Run:"

	# Extra horizontal padding between prompt and input.
	prompt-padding = 10

	# Placeholder input text.
	placeholder-text = ""

	# Maximum number of results to display.
	num-results = 0

	# Spacing between results in pixels. Can be negative.
	result-spacing = 1.5

	# List results horizontally.
	horizontal = false

	# Minimum width of input in horizontal mode.
	min-input-width = 0

	# Width and height of the window. Can be pixels or a percentage.
	width = 300
	height = 720

	# Window background color
	background-color = #1B1D1E

	# Width of the border outlines in pixels.
	outline-width = 2

	# Border outline color
	outline-color = #202020

	# Width of the border in pixels.
	border-width = 4

	# Border color
	border-color = #0926aa

	# Radius of window corners in pixels.
	corner-radius = 6

	# Padding between borders and text. Can be pixels or a percentage.
	padding-top = 12
	padding-bottom = 6
	padding-left = 8
	padding-right = 0

	clip-to-padding = true

	# Whether to scale the window by the output's scale factor.
	scale = true

	# default output chosen by the compositor.
	output = ""

	# Location on screen to anchor the window to.
	anchor = right

	# Set the size of the exclusive zone.

	exclusive-zone = -1

	# Window offset from edge of screen.
    margin-top = 0
	margin-bottom = 0
	margin-left = 0
	margin-right = 0

	# Hide the mouse cursor.
	hide-cursor = false

	# Show a text cursor in the input field.
	text-cursor = true

	# Sort results by number of usages in run and drun modes.
	history = false

	# Supported values: normal, prefix, fuzzy
	matching-algorithm = fuzzy

	require-match = true

	auto-accept-single = false

	hide-input = false

	hidden-character = "*"

	physical-keybindings = true

	print-index = false

	# If true, directly launch applications on selection when in drun mode.
	# Otherwise, just print the command line to stdout.
	drun-launch = true

	late-keyboard-init = false
	multi-instance = false
	ascii-input = false







